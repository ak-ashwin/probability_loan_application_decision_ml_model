{
  "prod": {
    "app_function": "wsgi_decision_ml.application",
    "aws_region": "ap-south-1",
    "project_name": "probability_loan_decision_ml_model",
    "runtime": "python3.6",
    "s3_bucket": "probability-loan-decision-ml-model-prod",
    "api_key_required": false,
    "slim_handler": true,
    "keep_warm": false,
    "environment_variables": {
      "env": "prod"
    },
    "extra_permissions": [
      {
        "Effect": "Allow",
        "Action": [
          "secretsmanager:*"
        ],
        "Resource": "*"
      }
    ],
    "memory_size": 1024,
    "timeout_seconds": 300
  }
}